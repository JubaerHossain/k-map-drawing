<%- include('../partials/head') %>
    <body>

        <div class="container-fluidtext-center">
        <%- include('../partials/header') %>
        </div>
    <div class="container-fluid p-5 bg-primary text-white text-center">
        <h1>File upload list</h1>
    </div>

    <div class="container mt-5">
        <div class="d-flex justify-content-center">
            <div class="col-md-6">
                <h2 class="text-center">
                    <% if (errors?.message) { %>
                        <small class="pt-2 pb-2 text-danger"
                          ><%= errors?.message %></small
                        >
                        <% } %>
                </h2>
            </div>
        </div>
    </div>

    <div class="container mt-5">
        <div class="d-flex justify-content-center">
            
             <div class="col-md-12">
                
                <table className="table">
                    <thead>
                        <tr>    
                            <th scope="col">#</th>
                            <th scope="col">Location details</th>
                            <th scope="col">Location employee details</th>
                            <th scope="col">Employee details</th>
                            <th scope="col">Colleges location values</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(let i = 0; i < data.length; i++) { %>
                        <tr>
                            <th scope="row"><%= i+1 %></th>
                            <td><%= data[i].location_details %></td>
                            <td><%= data[i].location_employee_details %></td>
                            <td><%= data[i].employee_details %></td>
                            <td><%= data[i].colleges_location_values %></td>
                            <td>
                                <a href="/api/v1/download?id=<%= data[i]._id %>" class="btn btn-primary">Download Sample Output</a>
                            </td>
                        </tr>
                        <% } %>
                        
                    </tbody>
                </table>
             </div>
        </div>
    </div>

    </body>
<%- include('../partials/footer') %>